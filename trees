clear()
size = get_world_size()
while(True):
	for x in range(size):
		for y in range(size):
			if can_harvest():
				harvest()
			if (((size - get_pos_x()) % 2) == 0):
				if (((size - get_pos_y()) % 2) == 0):
					plant(Entities.Tree)
				else:
					plant(Entities.Bush)
			else:
				if (((size - get_pos_y()) % 2) == 0):
					plant(Entities.Bush)
				else:
					plant(Entities.Tree)
			move(North)
		move(East)